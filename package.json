{
  "name": "huno-server",
  "version": "1.0.3",
  "description": "Node server app library",
  "author": "G'olibjon Narzullayev",
  "license": "MIT",
  "homepage": "https://github.com/golibnarzullayev/huno#readme",
  "keywords": ["mini", "app", "huno-server"],
